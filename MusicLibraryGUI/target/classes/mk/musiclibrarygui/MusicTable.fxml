<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mk.musiclibrarygui.MusicTableController">
   <children>
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#addNewSong" prefHeight="75.0" prefWidth="150.0" text="Add song">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Button>
      <Button layoutX="275.0" layoutY="361.0" mnemonicParsing="false" onAction="#removeSong" prefHeight="75.0" prefWidth="150.0" text="Remove song">
         <font>
            <Font name="Comic Sans MS" size="19.0" />
         </font>
      </Button>
      <Button layoutX="536.0" layoutY="361.0" mnemonicParsing="false" onAction="#goBack" prefHeight="75.0" prefWidth="150.0" text="Go back">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Button>
      <TableView fx:id="table" layoutX="12.0" layoutY="14.0" prefHeight="306.0" prefWidth="676.0">
        <columns>
            <TableColumn fx:id="songID" prefWidth="35.0" resizable="false" text="ID" />
            <TableColumn fx:id="songTitle" prefWidth="115.0" resizable="false" text="Title" />
            <TableColumn fx:id="authorName" prefWidth="115.0" resizable="false" text="Name" />
            <TableColumn fx:id="authorSurname" prefWidth="115.0" resizable="false" text="Surname" />
            <TableColumn fx:id="songAlbum" prefWidth="115.0" resizable="false" text="Album" />
            <TableColumn fx:id="songRelease" prefWidth="115.0" resizable="false" text="Release" />
            <TableColumn fx:id="songTime" prefWidth="65.0" resizable="false" text="Time" />
        </columns>
      </TableView>
      <Label fx:id="errorLabel" layoutX="205.0" layoutY="320.0" prefHeight="38.0" prefWidth="479.0" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="Comic Sans MS" size="27.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
